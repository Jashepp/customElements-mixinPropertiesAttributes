{
  "name": "ce-mixinprops",
  "version": "1.7.0",
  "description": "Mixin for Custom Elements (Web Components) to handle/sync properties and attributes",
  "main": "index.js",
  "author": "unchosen.aus@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jashepp/customElements-mixinPropertiesAttributes"
  },
  "devDependencies": {
    "cypress": "^9.5.1",
    "http-server": "^14.1.0",
    "nodemon": "^2.0.15",
    "start-server-and-test": "^1.14.0"
  },
  "scripts": {
    "server": "npx --node-options=--no-deprecation http-server ./ -s -c-1 -p 8181",
    "cypress": "npx cypress open",
    "cypress+server": "npx start-server-and-test server http-get://localhost:8181 cypress",
    "test": "npx start-server-and-test server http-get://localhost:8181 'npx cypress run'",
    "watch:test": "npx start-server-and-test server http-get://localhost:8181 watch:test:cypress",
    "watch:test:cypress": "npx nodemon --watch ./ --watch ./tests/ --exec cypress run"
  }
}
